{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users",
        "dest": "/routes/users.js"
      },
      {
        "src": "/api/links",
        "dest": "/routes/links.js"
      },
      {
        "src": "/api/re",
        "dest": "/routes/redirect.js"
      }
    ]
  }
  